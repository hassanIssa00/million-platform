{
    "name": "million-edtech-backend",
    "version": "1.0.0",
    "description": "Million EdTech Platform - Prisma Backend",
    "main": "dist/server.js",
    "scripts": {
        "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
        "build": "tsc",
        "start": "node dist/server.js",
        "db:migrate": "prisma migrate dev",
        "db:generate": "prisma generate",
        "db:push": "prisma db push",
        "db:seed": "ts-node prisma/seed.ts",
        "db:studio": "prisma studio",
        "worker": "ts-node src/workers/index.ts",
        "lint": "eslint . --ext .ts",
        "test": "jest"
    },
    "keywords": [
        "edtech",
        "prisma",
        "postgres",
        "nodejs"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "@prisma/client": "^5.8.0",
        "bcrypt": "^5.1.1",
        "bullmq": "^5.0.0",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-async-errors": "^3.1.1",
        "express-validator": "^7.0.1",
        "helmet": "^7.1.0",
        "ioredis": "^5.3.2",
        "jsonwebtoken": "^9.0.2",
        "morgan": "^1.10.0",
        "stripe": "^14.10.0",
        "winston": "^3.11.0"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.2",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/jsonwebtoken": "^9.0.5",
        "@types/morgan": "^1.9.9",
        "@types/node": "^20.10.6",
        "@typescript-eslint/eslint-plugin": "^6.17.0",
        "@typescript-eslint/parser": "^6.17.0",
        "eslint": "^8.56.0",
        "jest": "^29.7.0",
        "prisma": "^5.8.0",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.3.3"
    }
}